{
	"plugins": [
		"babel-plugin-styled-components",
		[
			"@emotion",
			{
				"importMap": {
					"@mui/system": {
						"styled": {
							"canonicalImport": ["@emotion/styled", "default"],
							"styledBaseImport": ["@mui/system", "default"]
						}
					},
					"@mui/material/styles": {
						"styled": {
							"canonicalImport": ["@emotion/styled", "default"],
							"styledBaseImport": ["@mui/material/styles", "styled"]
						}
					}
				}
			}
		]
	]
}
